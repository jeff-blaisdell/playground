{
    "node": true,
    "browser": true,
    "esnext": true,
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "indent": 2,
    "newcap": true,
    "noarg": true,
    "quotmark": "single",
    "undef": true,
    "unused": "vars",
    "strict": true,
    "trailing": true,
    "smarttabs": true,
    "predef": [
        "_",
        "afterEach",
        "angular",
        "beforeEach",
        "console",
        "describe",
        "ddescribe",
        "expect",
        "inject",
        "it",
        "iit",
        "jasmine",
        "module",
        "spyOn",
        "xdescribe",
        "xit"
    ]
}
